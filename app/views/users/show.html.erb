<h1>User Created Events</h1>
<table>
  <tr>
    <th>Event Name</th>
    <th>Event Place</th>
    <th>Event Time</th>
  </tr>
  <% @events.each do |e| %>
    <tr>
      <td>
        <%= e.event_name %>
      </td>
      <td>
        <%= e.event_place %>
      </td>
      <td>
        <%= e.event_date %>
      </td>
    </tr>
  <% end %>
</table>
